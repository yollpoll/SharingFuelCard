<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="StyleBtnLogin">
        <item name="android:background">@drawable/selector_btn_login</item>
    </style>

    <style name="StyleApplyFuelCard">
        <item name="android:background">@drawable/selector_apply_fuel_card</item>
    </style>

    <style name="StyleWhiteRipple"></style>
    <style name="StyleAddPhoto">
        <item name="android:background">#eeeeee</item>
    </style>
</resources>
